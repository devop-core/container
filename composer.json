{
    "name": "devop-core/container",
    "description": "Dependency injection service",
    "keywords": ["php", "container", "dependency-injection"],
    "homepage": "https://www.devop.studio",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "DevOp Studio",
            "email": "core@devop.studio",
            "homepage": "https://www.devop.studio"
        },
        {
            "name": "Zlatin Hristov",
            "email": "about@z-latko.info",
            "homepage": "https://z-latko.info",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "provide": {
        "psr/container-implementation": "^1.0"
    },
    "autoload": {
        "psr-4": {
